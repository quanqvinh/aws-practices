{
  "name": "aws-practices",
  "version": "0.1.0",
  "description": "AWS CDK practices",
  "repository": "https://github.com/quanqvinh/aws-practices.git",
  "license": "MIT",
  "author": "quanqvinh <nguyenquanqvinh@gmail.com>",
  "main": "index.js",
  "bin": {
    "aws-practices": "bin/aws-practices.js"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "commit": "git-cz",
    "commitlint": "commitlint --edit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "node .husky/install.mjs",
    "prettier": "prettier --write .",
    "test": "jest",
    "watch": "tsc -w"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "eslint --fix --cache",
    "*.{js,mjs,cjs,ts,tsx}": "prettier --write --list-different --ignore-unknown",
    "package.json": "npx sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "aws-cdk-lib": "2.162.1",
    "constructs": "^10.0.0",
    "dotenv": "^16.4.5",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@eslint/js": "^9.13.0",
    "@types/jest": "^29.5.12",
    "@types/node": "22.5.4",
    "aws-cdk": "2.162.1",
    "commitizen": "^4.3.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cdk": "^1.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "sort-package-json": "^2.10.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0"
  }
}
